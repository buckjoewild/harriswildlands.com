# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@db:5432/harriswildlands
# For local development without Docker, use:
# DATABASE_URL=postgresql://user:password@localhost:5432/harriswildlands

# Session Security
SESSION_SECRET=change-this-to-random-string-in-production

# Server Port (optional, defaults to 5000)
PORT=5000

# AI Provider Configuration
# Options: gemini | openrouter | off
# Priority: If gemini fails, falls back to openrouter, then to off
AI_PROVIDER=off

# Optional AI API Keys
# Gemini (Google AI Studio - free tier available)
GOOGLE_GEMINI_API_KEY=

# OpenRouter (paid, supports multiple models)
OPENROUTER_API_KEY=
